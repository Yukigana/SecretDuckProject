{% extends '/Layouts/header_and_menu.html.twig' %}

{% block title %}{{ parent() }} : form{% endblock %}

{% block main_title %}Vue "film_add" : exercices formulaires{% endblock %}

{% block corps %}
    <h2>Ajout d'un Produit</h2>

    {{ form_start(myform) }}

    {{ form_errors(myform) }}

    {{ form_row(myform.libelle) }}

    {{ form_rest(myform) }}

    {{ form_end(myform) }}

    <h2><a href="{{ path('overview_list') }}"><button class="mon-button" > Retour liste des produits </button></a></h2>

    <h2>dump</h2>
    {{ dump(myform) }}
{% endblock %}
